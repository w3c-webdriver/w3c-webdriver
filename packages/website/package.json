{
  "name": "w3c-webdriver-website",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:mucsi96/w3c-webdriver.git",
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "add-contributor": "all-contributors add",
    "build": "del-cli dist && webpack --mode production && del-cli dist/site-server-renderer.js",
    "serve": "serve dist",
    "start": "webpack-dev-server --mode development",
    "deploy": "gh-pages --dist dist --repo git@github.com:w3c-webdriver/w3c-webdriver.github.io.git --branch master",
    "audit": "lighthouse https://w3c-webdriver.github.io/ --output-path=./lighthouse.report.html --quiet --view"
  },
  "dependencies": {
    "typedoc": "^0.14.2",
    "styled-components": "^4.3.2",
    "react-syntax-highlighter": "^11.0.1",
    "react-markdown": "^4.1.0",
    "highlight.js": "^9.15.8",
    "@sindresorhus/slugify": "^0.9.1",
    "react-router": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "docsearch.js": "^2.6.3",
    "react-scrolllock": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.0.0",
    "del-cli": "^2.0.0",
    "emoji-regex": "^8.0.0",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.4",
    "json-loader": "^0.5.7",
    "lighthouse": "^5.1.0",
    "loader-utils": "^1.2.3",
    "netlify-cli": "^2.11.23",
    "serve": "^11.1.0",
    "simple-icons": "^1.10.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^0.23.1",
    "twemoji": "12.1.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "defaults",
    "not IE > 0",
    "not IE_Mob > 0"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true
        }
      ],
      "inline-react-svg"
    ]
  }
}
